! Configuração de Hostname e Acesso
hostname S05-SW-C9288
enable secret cisco123
line console 0
 password cisco
 login
line vty 0 4
 password cisco
 login
 transport input ssh
!
ip domain-name lab.local
crypto key generate rsa modulus 2048
username admin secret admin123
!
interface Vlan10
 ip address 10.0.10.5 255.255.255.0
interface Vlan20
 ip address 10.0.20.5 255.255.255.0
interface Vlan30
 ip address 10.0.30.5 255.255.255.0
!
! Porta Uplink para outro switch
interface Gig1/1
 description Uplink-to-S06
 switchport mode trunk
 switchport trunk allowed vlan 10,20,30
!
! Porta para servidor
interface Gig1/10
 description S05-SERVER-01
 switchport mode access
 switchport access vlan 20
 spanning-tree portfast
!
ip routing
!
vlan 10
 name MGMT
vlan 20
 name DATA
vlan 30
 name STORAGE
!
end
